{
    "name": "@revertdotdev/revert-vue",
    "version": "0.0.7",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/revertinc/Revert.git"
    },
    "scripts": {
        "dev": "vite --host",
        "serve": "vite preview",
        "build": "vue-tsc --noEmit && vite build",
        "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
        "publish": "npm publish --access public"
    },
    "license": "UNLICENSED",
    "peerDependencies": {
        "vue": "^3.0.0"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/revert-vue.umd.js",
    "module": "./dist/revert-vue.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/revert-vue.es.js",
            "require": "./dist/revert-vue.umd.js"
        }
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "dependencies": {
        "vite-plugin-dts": "latest",
        "vue": "^3.2.13",
        "vue-tsc": "^2.0.29"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "@vitejs/plugin-vue": "^4.3.4",
        "@vue/cli-plugin-babel": "~5.0.0",
        "@vue/cli-plugin-eslint": "~5.0.0",
        "@vue/cli-plugin-typescript": "~5.0.0",
        "@vue/cli-service": "~5.0.0",
        "@vue/eslint-config-typescript": "^9.1.0",
        "eslint": "^8.50.0",
        "eslint-plugin-vue": "^9.17.0",
        "typescript": "~4.5.5",
        "vite": "^4.4.9",
        "workspace-tools": "^0.30.0"
    }
}
